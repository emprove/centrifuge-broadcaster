{
    "name": "emprove/centrifuge-broadcaster",
    "type": "library",
    "description": "Centrifugo broadcaster for laravel ^5.8 | ^6.0",
    "keywords": ["laravel", "centrifugo", "broadcaster"],
    "license": "MIT",
    "authors": [
        {
            "name": "Emprove",
            "email": "emprove@pm.me"
        }
    ],
    "homepage": "https://github.com/emprove/centrifuge-broadcaster",
    "support": {
        "issues": "https://github.com/emprove/centrifuge-broadcaster/issues",
        "source": "https://github.com/emprove/centrifuge-broadcaster"
    },
    "require": {
        "php": "^7.1.3",
        "illuminate/broadcasting": "^5.8|^6.0",
        "guzzlehttp/guzzle": "^6.3.3",
        "lcobucci/jwt": "^3.2"
    },
    "require-dev": {
        "orchestra/testbench": "^3.3",
        "phpunit/phpunit": "^5.7|^6.1"
    },
    "autoload": {
        "psr-4": {
            "Emprove\\Centrifugo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emprove\\Centrifugo\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Emprove\\Centrifugo\\CentrifugoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

